{
  "name": "kineo",
  "version": "0.5.0",
  "description": "Declarative, TypeScript-first Object-Graph Mapper.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schema": {
      "default": "./dist/schema.js",
      "types": "./dist/schema.d.ts"
    },
    "./model": {
      "default": "./dist/model.js",
      "types": "./dist/model.d.ts"
    },
    "./ir": {
      "default": "./dist/ir.js",
      "types": "./dist/ir.d.ts"
    },
    "./adapter": {
      "default": "./dist/adapter.js",
      "types": "./dist/adapter.d.ts"
    },
    "./adapters/neo4j": {
      "default": "./dist/adapters/neo4j.js",
      "types": "./dist/adapters/neo4j.d.ts"
    },
    "./compilers/cypher": {
      "default": "./dist/compilers/cypher.js",
      "types": "./dist/compilers/cypher.d.ts"
    }
  },
  "keywords": [
    "object-graph-mapper",
    "ogm",
    "graph-database",
    "graph-db",
    "object-relation-mapper",
    "orm",
    "sql-database",
    "sql-db"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "trailfrost",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "neo4j-driver": "^5.28.1"
  }
}
