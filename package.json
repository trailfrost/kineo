{
  "name": "kineo",
  "version": "0.6.0",
  "type": "module",
  "description": "Declarive, TypeScript-first Object-Relation/Graph Mapper (ORM/OGM).",
  "main": "./dist/client.js",
  "types": "./dist/client.d.ts",
  "bin": "./dist/kit/cli.js",
  "engines": {
    "node": ">= 20.5"
  },
  "exports": {
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./adapter": {
      "types": "./dist/adapter/index.d.ts",
      "default": "./dist/adapter/index.js"
    },
    "./adapter/neo4j": {
      "types": "./dist/adapter/neo4j.d.ts",
      "default": "./dist/adapter/neo4j.js"
    },
    "./ir": {
      "types": "./dist/ir.d.ts",
      "default": "./dist/ir.js"
    },
    "./model": {
      "types": "./dist/model.d.ts",
      "default": "./dist/model.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./dist/schema.js"
    },
    "./kit": {
      "types": "./dist/kit/index.d.ts",
      "default": "./dist/kit/index.js"
    },
    "./kit/cli": {
      "types": "./dist/kit/cli.d.ts",
      "default": "./dist/kit/cli.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest tests",
    "format": "prettier --check .",
    "format!": "prettier --write .",
    "kit": "jiti src/kit/cli.ts",
    "lint": "eslint .",
    "lint!": "eslint --fix .",
    "check": "tsc -b --noEmit",
    "build": "tsc -b tsconfig.app.json && tsc-alias -p tsconfig.app.json"
  },
  "keywords": [
    "ogm",
    "orm",
    "object-graph-mapper",
    "object-relation-mapper"
  ],
  "author": "trailfrost",
  "license": "MIT",
  "packageManager": "pnpm@10.20.0",
  "dependencies": {
    "convoker": "^0.3.2",
    "jiti": "^2.5.1"
  },
  "optionalDependencies": {
    "neo4j-driver": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.5.2",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vitest": "^4.0.5"
  }
}
