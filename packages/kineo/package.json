{
  "name": "kineo",
  "version": "0.9.1",
  "type": "module",
  "description": "Declarive, TypeScript-first Object-Relation/Graph Mapper (ORM/OGM).",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "kineo": "./dist/main.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trailfrost/kineo"
  },
  "engines": {
    "node": ">= 20.19.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./ir": {
      "types": "./dist/ir.d.mts",
      "import": "./dist/ir.mjs"
    },
    "./kit": {
      "types": "./dist/kit/index.d.mts",
      "import": "./dist/kit/index.mjs"
    },
    "./neo4j": {
      "types": "./dist/adapters/neo4j.d.mts",
      "import": "./dist/adapters/neo4j.mjs"
    },
    "./cypher": {
      "types": "./dist/compilers/cypher.d.mts",
      "import": "./dist/compilers/cypher.mjs"
    },
    "./sql": {
      "types": "./dist/compilers/sql.d.mts",
      "import": "./dist/compilers/sql.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "kit": "jiti src/kit/main.ts",
    "check": "tsc -b",
    "build": "tsdown",
    "test": "vitest"
  },
  "keywords": [
    "ogm",
    "orm",
    "object-graph-mapper",
    "object-relation-mapper"
  ],
  "author": "trailfrost",
  "license": "MIT",
  "dependencies": {
    "convoker": "^0.3.4",
    "jiti": "^2.5.1"
  },
  "optionalDependencies": {
    "neo4j-driver": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "~20.19.0",
    "tsdown": "^0.18.0",
    "typescript": "^5.9.2"
  }
}
