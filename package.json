{
  "name": "kineo",
  "version": "0.6.0",
  "type": "module",
  "description": "Declarive, TypeScript-first Object-Relation/Graph Mapper (ORM/OGM).",
  "main": "./dist/client.mjs",
  "types": "./dist/client.d.mts",
  "bin": {
    "kineo": "./dist/kit/main.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trailfrost/kineo"
  },
  "engines": {
    "node": ">= 20.5"
  },
  "exports": {
    "./client": {
      "types": "./dist/client.d.mts",
      "import": "./dist/client.mjs"
    },
    "./error": {
      "types": "./dist/error.d.mts",
      "import": "./dist/error.mjs"
    },
    "./adapter": {
      "types": "./dist/adapter/index.d.mts",
      "import": "./dist/adapter/index.mjs"
    },
    "./adapter/neo4j": {
      "types": "./dist/adapter/neo4j.d.mts",
      "import": "./dist/adapter/neo4j.mjs"
    },
    "./compiler/cypher": {
      "types": "./dist/compiler/cypher.d.mts",
      "import": "./dist/compiler/cypher.mjs"
    },
    "./ir": {
      "types": "./dist/ir.d.mts",
      "import": "./dist/ir.mjs"
    },
    "./model": {
      "types": "./dist/model.d.mts",
      "import": "./dist/model.mjs"
    },
    "./schema": {
      "types": "./dist/schema.d.mts",
      "import": "./dist/schema.mjs"
    },
    "./kit": {
      "types": "./dist/kit/index.d.mts",
      "import": "./dist/kit/index.mjs"
    },
    "./kit/cli": {
      "types": "./dist/kit/cli.d.mts",
      "import": "./dist/kit/cli.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest tests",
    "format": "prettier --check .",
    "format!": "prettier --write .",
    "kit": "jiti src/kit/main.ts",
    "lint": "eslint .",
    "lint!": "eslint --fix .",
    "check": "tsc -b tsconfig.app.json",
    "build": "tsdown"
  },
  "keywords": [
    "ogm",
    "orm",
    "object-graph-mapper",
    "object-relation-mapper"
  ],
  "author": "trailfrost",
  "license": "MIT",
  "packageManager": "pnpm@10.20.0",
  "dependencies": {
    "convoker": "^0.3.4",
    "jiti": "^2.5.1"
  },
  "optionalDependencies": {
    "neo4j-driver": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.5.2",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.16.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vitest": "^4.0.5"
  }
}
